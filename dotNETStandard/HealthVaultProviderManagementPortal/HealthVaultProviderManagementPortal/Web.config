<configuration>
  <configSections/>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0"/>
    <add key="webpages:Enabled" value="false"/>
    <add key="ClientValidationEnabled" value="true"/>
    <add key="UnobtrusiveJavaScriptEnabled" value="true"/>
    <!-- BEGIN config settings used by the HealthVault SDK -->
    <add key="HV_ApplicationId" value="your application ID here"/>
    <add key="HV_ShellUrl" value="https://account.healthvault-ppe.com/"/>
    <add key="HV_HealthServiceUrl" value="https://platform.healthvault-ppe.com/platform/"/>
    <add key="HV_RestHealthServiceUrl" value="https://data.ppe.microsofthealth.net/"/>
    <!-- <add key="ApplicationCertificateFileName" value="absolute path to cert" />-->
    <!-- when we call the SignOut() method on HealthServicePage, it redirects us to the page below -->
    <add key="HV_NonProductionActionUrlRedirectOverride" value="Redirect"/>
    <!-- The redirect page (specified above) uses these keys below to redirect to different
         pages based on the response from the shell -->
    <add key="HV_ActionHome" value="Home"/>
    <add key="HV_ActionAppAuthReject" value="Home"/>
    <add key="HV_SSLForSecure" value="false"/>
    <!-- Comma seperated list of person IDs that can access the offline pages. Open to all if empty -->
    <add key="HV_OfflineAuthorizedPersonIDs" value=""/>
    <!-- END config settings used by the HealthVault SDK -->
  </appSettings>
  <system.web>
    <authentication mode="None"/>
    <customErrors mode="On"/>
    <compilation debug="true" targetFramework="4.6.1"/>
    <httpRuntime targetFramework="4.5.2" requestValidationMode="2.0"/>
    <httpModules>
      <add name="AuthenticationModule" type="Microsoft.HealthVault.Web.AuthenticationModule"/>
    </httpModules>
  </system.web>
  <system.net>
    <!-- Uncomment this section to see outgoing calls in Fiddler
    <defaultProxy>
      <proxy usesystemdefault="False" bypassonlocal="True" proxyaddress="http://127.0.0.1:8888" />
    </defaultProxy>
    -->
  </system.net>
  <system.webServer>
    <modules>
      <remove name="FormsAuthentication"/>
      <add name="AuthenticationModule" type="Microsoft.HealthVault.Web.AuthenticationModule, Microsoft.HealthVault.Web" preCondition="managedHandler"/>
    </modules>
    <validation validateIntegratedModeConfiguration="false"/>
  </system.webServer>
